    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@prisma/client": "^3.5.0",
    "apollo-server": "^3.1.1",
    "bcryptjs": "~2.4.0",
    "graphql": "^15.5.1",
    "graphql-scalars": "^1.14.1",
    "jsonwebtoken": "~8.5.0",
    "nexus": "^1.1.0"
  }
}
